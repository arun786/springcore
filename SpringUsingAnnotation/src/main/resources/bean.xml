	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<context:annotation-config></context:annotation-config>

	<bean id="duke" class="com.arun.springidols.DukePerformer"></bean>
	<bean id="dukeAdriot" class="com.arun.springidols.DukeAdriotPerformer"></bean>
	<bean id="dukeGamer" class="com.arun.springidols.DukeTheGamer"></bean>

	<bean id="saxophone" class="com.arun.springidols.Saxophone"></bean>
	<bean id="guitar" class="com.arun.springidols.Guitar"></bean>
	<bean id="cricket" class="com.arun.springidols.Cricket"></bean>

</beans>
